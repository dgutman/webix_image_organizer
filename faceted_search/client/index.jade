        link(type="text/css" rel="stylesheet" href="libs/webix/skins/" + data.skin + ".css")
    link(type="text/css" rel="stylesheet" href="assets/app.css")
    //- Webix Library
    script(type="text/javascript" src="libs/webix/webix.min.js")
    script(type="text/javascript" src="libs/socket.io-stream/socket.io-stream.js")
    //- Development only
    script(type="text/javascript" src="libs/less/dist/less.min.js")
    //- The app's logic
    script(type="text/javascript" data-main="app" src="libs/requirejs/require.js")
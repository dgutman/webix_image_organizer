    link(type="text/css" rel="stylesheet" href="https://cdn.materialdesignicons.com/3.4.93/css/materialdesignicons.css" charset="utf-8")
    //- Styles
    link(type="text/css" rel="stylesheet" href="libs/webix/skins/" + data.skin + ".css")

{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/defaultNpSchema.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "npSchema"
    ],
    "properties": {
        "npSchema": {
            "type": "object",
            "default": {},
            "title": "The npSchema Schema",
            "required": [
                "blockID",
                "caseID",
                "stainID"
            ],
            "properties": {
                "blockID": {
                    "type": "string",
                    "default": "",
                    "title": "The blockID Schema",
                    "examples": [
                        "10"
                    ]
                },
                "caseID": {
                    "type": "string",
                    "default": "",
                    "title": "The caseID Schema",
                    "examples": [
                        "E18-7"
                    ],
                    "pattern": "^(E|A)\\d\\d-\\d+"
                },
                "stainID": {
                    "type": "string",
                    "default": "",
                    "title": "The stainID Schema",
                    "examples": [
                        "Tau"
                    ],
                    "enum": [
                        "pTDP",
                        "HE",
                        "aBeta",
                        "Ubiq",
                        "Tau",
                        "Biels",
                        "Syn",
                        "p62",
                        "LFB",
                        "FUS",
                        "TDP-43",
                        "GFAP"
                    ]
                }
            },
            "examples": [{
                "blockID": "10",
                "caseID": "E18-7",
                "stainID": "Tau"
            }]
        }
    },
    "examples": [{
        "npSchema": {
            "blockID": "10",
            "caseID": "E18-7",
            "stainID": "Tau"
        }
    }]
}

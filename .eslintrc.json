{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "webix": true,
    "APPNAME": true,
    "VERSION": true,
    "PRODUCTION": true,
    "$$": true,
    "console": true,
    "require": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-extra-parens": "warn",
    "valid-jsdoc": "warn",
    "no-console": "off"
  }
}